{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery"
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "lib/jquery-validate",
      "files": [
        "localization/messages_pt_BR.js",
        "localization/methods_pt.js",
        "additional-methods.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "lib/jquery-validation-unobtrusive",
      "files": [
        "jquery.validate.unobtrusive.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.6.0",
      "files": [
        "js/bootstrap.js",
        "css/bootstrap.css",
        "css/bootstrap-reboot.css",
        "css/bootstrap-grid.css"
      ],
      "destination": "lib/bootstrap"
    }
  ]
}